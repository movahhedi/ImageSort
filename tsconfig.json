{
	"compilerOptions": {
		"noImplicitAny": false,
		"target": "ES2022",
		"module": "es2022",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"moduleResolution": "node",
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"jsx": "react-jsx",
		"jsxImportSource": "lestin",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"types": [
			"vite/client",
			"jquery"
		]
	}
}
